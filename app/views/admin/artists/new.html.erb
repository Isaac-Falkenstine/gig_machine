<h1>Create a new artist</h1>

<section>
<%= form_for [:admin, @artist] do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :bio %>
<%= f.text_field :bio %>
<%= f.label :link %>
<%= f.text_field :link %>
<%= f.label :img %>
<%= f.text_field :img %>
<%= f.submit %>
<% end %>
</section>
