<h1><%= "Edit #{@artist.name}" %>

  <section>
    <%= form_for [:admin, @artist] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <%= f.label :link %>
    <%= f.text_field :link %>
    <%= f.label :img %>
    <%= f.text_field :img %>
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
    <%= f.submit %>
    <% end %>
  </section>
